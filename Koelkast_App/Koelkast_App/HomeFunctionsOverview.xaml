<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Koelkast_App.HomeFunctionsOverview">
    <ContentPage.Content>
        <StackLayout Margin="10, 0">
            <Button x:Name="ChooseDrink" Text="Drankje kiezen" Clicked="ChooseDrink_Clicked"/>
            <Button x:Name="Personalinfo" Text="Persoonlijke info" Clicked="Personalinfo_Clicked"/>
            <Button x:Name="Stock" Text="Stock" Clicked="Stock_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>